# Exemplo de configuração para StreamPay
# Preencha com seus dados reais para produção

POSTGRES_URL=postgres://usuario:senha@localhost:5432/streampay
ETH_RPC_URL=http://localhost:8545
PRIVATE_KEY=0x0123456789012345678901234567890123456789012345678901234567890123
STREAMPAY_CORE_ADDRESS=0xSeuEnderecoDoContrato

# --- Portas padrão ---
PORT_BACKEND=3001
PORT_FRONTEND=3003
ELIZA_SERVER_PORT=3002

# URL pública usada pelo frontend para acessar o backend
NEXT_PUBLIC_BACKEND_URL=http://localhost:3001

# --- Provedores de IA ---
# Se ambos estiverem definidos, o Google é usado por padrão
GOOGLE_API_KEY=
GEMINI_API_KEY=
